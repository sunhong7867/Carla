<?xml version="1.0"?>
<OpenSCENARIO>

  <FileHeader revMajor="1" revMinor="0"
              date="2025-05-24T12:00:00"
              description="TS_ACC_S005: Straight road, front-moving + side cut-in"
              author="YCS"/>

  <ParameterDeclarations>
    <!-- 속도 파라미터 -->
    <ParameterDeclaration name="k60"  parameterType="double" value="16.67"/>
    <!-- 초기 위치 -->
    <ParameterDeclaration name="egoX"    parameterType="double" value="0"/>
    <ParameterDeclaration name="egoY"    parameterType="double" value="0"/>
    <ParameterDeclaration name="frontX"  parameterType="double" value="100"/>
    <ParameterDeclaration name="frontY"  parameterType="double" value="0"/>
    <ParameterDeclaration name="ciX"     parameterType="double" value="10"/>
    <ParameterDeclaration name="ciY"     parameterType="double" value="3.5"/>
    <ParameterDeclaration name="zHeight" parameterType="double" value="28"/>
    <!-- 날씨 -->
    <ParameterDeclaration name="cloud_state"       parameterType="string" value="free"/>
    <ParameterDeclaration name="weather_type"      parameterType="string" value="rain"/>
    <ParameterDeclaration name="weather_intensity" parameterType="double" value="1.0"/>
  </ParameterDeclarations>

  <CatalogLocations/>

  <RoadNetwork>
    <LogicFile filepath="Map1"/>
  </RoadNetwork>

  <Entities>
    <!-- Ego 차량 (중앙 차선) -->
    <ScenarioObject name="ego">
      <Vehicle name="vehicle.pass.orange.k5" vehicleCategory="car">
        <Performance maxSpeed="69.444" maxAcceleration="10" maxDeceleration="10"/>
        <BoundingBox>
          <Center x="1.5" y="0"   z="0.9"/>
          <Dimensions width="2.1" length="4.5" height="1.8"/>
        </BoundingBox>
        <Axles>
          <FrontAxle maxSteering="0.5" wheelDiameter="0.6"
                     trackWidth="1.8" positionX="3.1" positionZ="0.3"/>
          <RearAxle  maxSteering="0"   wheelDiameter="0.6"
                     trackWidth="1.8" positionX="0"   positionZ="0.3"/>
        </Axles>
        <Properties><Property name="type" value="ego_vehicle"/></Properties>
      </Vehicle>
    </ScenarioObject>

    <!-- 정면 차량 (중앙 차선, 100m 앞) -->
    <ScenarioObject name="front">
      <Vehicle name="vehicle.pass.green.audi" vehicleCategory="car">
        <Performance maxSpeed="69.444" maxAcceleration="10" maxDeceleration="10"/>
        <BoundingBox>
          <Center x="1.5" y="0"   z="0.9"/>
          <Dimensions width="2.1" length="4.5" height="1.8"/>
        </BoundingBox>
        <Axles>
          <FrontAxle maxSteering="0.5" wheelDiameter="0.6"
                     trackWidth="1.8" positionX="3.1" positionZ="0.3"/>
          <RearAxle  maxSteering="0"   wheelDiameter="0.6"
                     trackWidth="1.8" positionX="0"   positionZ="0.3"/>
        </Axles>
        <Properties><Property name="type" value="simulation"/></Properties>
      </Vehicle>
    </ScenarioObject>

    <!-- Cut-in 차량 (왼쪽 차선, 50m 거리) -->
    <ScenarioObject name="cutin">
      <Vehicle name="vehicle.pass.brown.genesis" vehicleCategory="car">
        <Performance maxSpeed="69.444" maxAcceleration="10" maxDeceleration="10"/>
        <BoundingBox>
          <Center x="1.5" y="0"   z="0.9"/>
          <Dimensions width="2.1" length="4.5" height="1.8"/>
        </BoundingBox>
        <Axles>
          <FrontAxle maxSteering="0.5" wheelDiameter="0.6"
                     trackWidth="1.8" positionX="3.1" positionZ="0.3"/>
          <RearAxle  maxSteering="0"   wheelDiameter="0.6"
                     trackWidth="1.8" positionX="0"   positionZ="0.3"/>
        </Axles>
        <Properties><Property name="type" value="simulation"/></Properties>
      </Vehicle>
    </ScenarioObject>
  </Entities>

  <Storyboard>
    <Init>
      <Actions>
        <!-- 환경 -->
        <GlobalAction>
          <EnvironmentAction>
            <Environment name="Env">
              <TimeOfDay animation="false" dateTime="2025-05-24T12:00:00"/>
              <Weather cloudState="$cloud_state">
                <Sun intensity="10000" azimuth="0" elevation="1.57"/>
                <Fog visualRange="10000"/>
                <Precipitation precipitationType="$weather_type"
                               intensity="$weather_intensity"/>
              </Weather>
              <RoadCondition frictionScaleFactor="1.0"/>
            </Environment>
          </EnvironmentAction>
        </GlobalAction>

        <!-- Ego 설정 -->
        <Private entityRef="ego">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="$egoX" y="$egoY" z="$zHeight" h="0" p="0" r="0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <ControllerAction>
              <AssignControllerAction>
                <Controller name="EgoADAS">
                  <Properties>
                    <Property name="module" value="autonomous_driving_system"/>
                  </Properties>
                </Controller>
              </AssignControllerAction>
              <OverrideControllerValueAction>
                <Throttle value="0" active="false"/>
                <Brake   value="0" active="false"/>
                <Clutch  value="0" active="false"/>
                <ParkingBrake value="0" active="false"/>
                <SteeringWheel value="0" active="false"/>
                <Gear number="0" active="false"/>
              </OverrideControllerValueAction>
            </ControllerAction>
          </PrivateAction>
        </Private>

        <!-- 정면 차량 설정 -->
        <Private entityRef="front">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="$frontX" y="$frontY" z="$zHeight" h="0" p="0" r="0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step"
                                     dynamicsDimension="time"
                                     value="0"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="$k60"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>

        <!-- Cut-in 차량 설정 -->
        <Private entityRef="cutin">
          <PrivateAction>
            <TeleportAction>
              <Position>
                <WorldPosition x="$ciX" y="$ciY" z="$zHeight" h="0" p="0" r="0"/>
              </Position>
            </TeleportAction>
          </PrivateAction>
          <PrivateAction>
            <LongitudinalAction>
              <SpeedAction>
                <SpeedActionDynamics dynamicsShape="step"
                                     dynamicsDimension="time"
                                     value="0"/>
                <SpeedActionTarget>
                  <AbsoluteTargetSpeed value="$k60"/>
                </SpeedActionTarget>
              </SpeedAction>
            </LongitudinalAction>
          </PrivateAction>
        </Private>
      </Actions>
    </Init>

    <Story name="CutInStory">
      <Act name="ScenarioAct">

        <!-- 1) 2000m 구간 거리 기반 선형 가속 -->
        <ManeuverGroup maximumExecutionCount="1" name="SpeedupAll">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="front"/>
            <EntityRef entityRef="cutin"/>
          </Actors>
          <Maneuver name="AllSpeedUp">
            <Event name="AllSpeedUpEvent" priority="overwrite">
              <Action name="AllSpeedUpAction">
                <PrivateAction>
                  <LongitudinalAction>
                    <SpeedAction>
                      <SpeedActionDynamics dynamicsShape="linear"
                                            dynamicsDimension="distance"
                                            value="2000"/>
                      <SpeedActionTarget>
                        <AbsoluteTargetSpeed value="$k60"/>
                      </SpeedActionTarget>
                    </SpeedAction>
                  </LongitudinalAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartAll" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="0" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <!-- 2) 5초 후 Cut-in 차량 중앙 차선으로 차선 변경 -->
        <ManeuverGroup maximumExecutionCount="1" name="CutInGroup">
          <Actors selectTriggeringEntities="false">
            <EntityRef entityRef="cutin"/>
          </Actors>
          <Maneuver name="CutInManeuver">
            <Event name="CutInEvent" priority="overwrite">
              <Action name="LaneChangeToEgo">
                <PrivateAction>
                  <LateralAction>
                    <LaneChangeAction>
                      <LaneChangeActionDynamics dynamicsShape="linear"
                                                dynamicsDimension="distance"
                                                value="14.5"/>
                      <LaneChangeTarget>
                        <RelativeTargetLane entityRef="cutin" value="-1"/>
                      </LaneChangeTarget>
                    </LaneChangeAction>
                  </LateralAction>
                </PrivateAction>
              </Action>
              <StartTrigger>
                <ConditionGroup>
                  <Condition name="StartCutIn" delay="0" conditionEdge="rising">
                    <ByValueCondition>
                      <SimulationTimeCondition value="5" rule="greaterThan"/>
                    </ByValueCondition>
                  </Condition>
                </ConditionGroup>
              </StartTrigger>
            </Event>
          </Maneuver>
        </ManeuverGroup>

        <StartTrigger>
          <ConditionGroup>
            <Condition name="ActStart" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="0" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StartTrigger>

        <StopTrigger>
          <ConditionGroup>
            <Condition name="ActEnd" delay="0" conditionEdge="rising">
              <ByValueCondition>
                <SimulationTimeCondition value="20" rule="greaterThan"/>
              </ByValueCondition>
            </Condition>
          </ConditionGroup>
        </StopTrigger>

      </Act>
    </Story>

    <StopTrigger>
      <ConditionGroup>
        <Condition name="ScenarioEnd" delay="0" conditionEdge="rising">
          <ByValueCondition>
            <SimulationTimeCondition value="20" rule="greaterThan"/>
          </ByValueCondition>
        </Condition>
      </ConditionGroup>
    </StopTrigger>

  </Storyboard>
</OpenSCENARIO>
